Define PrintList(L);  Foreach A In L Do    PrintLn;  PrintLn(A);  PrintLn;EndForeach;EndDefine;
Define StampaBene(X,P);Foreach A In X Do PrintLn ;  Foreach B In A Do Print Format(B,P);  EndForeach; EndForeach; EndDefine;
Define Vert(M,N)   L:=[];  For K:=1 To N Do    If Type(M/x[K])=POLY Then Append(L,K);EndIf;  EndFor;  Return(L);EndDefine; 
Define Vrb(M,N);  L:=[];  For K:=1 To N Do    If Type(M/x[K])=POLY Then Append(L,x[K]);EndIf;  EndFor;  Return(L);EndDefine;
Define Int(N); M:=1; If N<0 Then Repeat M:=M-1; Until M<=N+1; Elif  N>=1 Then Repeat M:=M+1; Until M>N;EndIf; Return(M-1);EndDefine;
Define SPOL(F,G); P:=(LT(G)/(GCD(LT(F),LT(G))))F- (LT(F)/(GCD(LT(F),LT(G))))G; Return(P);EndDefine;
Define ComponentwiseDiff(A,B); If Len(A)=Len(B) Then Return([A[I]-B[I]|I In 1..Len(A)]);Else Return("Cannot add list of different lengths!");EndIf;EndDefine;
